<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="taskPane" prefHeight="57.0" prefWidth="620.0" styleClass="task" stylesheets="@todopage.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pathpilotfx.controller.todolist.TaskController">
<!--   prefHeight="57.0"-->
   <children>
      <CheckBox fx:id="checkboxField" layoutX="16.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleStatusChange" prefHeight="28.0" prefWidth="18.0" styleClass="checkbox" stylesheets="@todopage.css">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <Label fx:id="taskNameField" layoutX="50.0" layoutY="16.0" prefHeight="29.0" prefWidth="320.0" text="Label">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="taskPrioField" layoutX="500.0" layoutY="15.0" prefHeight="30.0" prefWidth="112.0" styleClass="taskPrio" stylesheets="@todopage.css" text="MEDIUM" textFill="#f8f8f8">
         <font>
            <Font size="17.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="taskDueDate" layoutX="380.0" layoutY="15.0" prefHeight="30.0" prefWidth="112.0" styleClass="taskPrio" stylesheets="@todopage.css" text="2024/11/28" textAlignment="CENTER" textFill="#f8f8f8">
         <font>
            <Font size="17.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
   </children>
</Pane>
